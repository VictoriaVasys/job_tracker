<h4>Create a new Contact at this company</h4>
<%= form_for [@company, @contact] do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br><br>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br><br>

  <%= f.label :position %>
  <%= f.text_field :position %><br><br>

  <%= f.label :email %>
  <%= f.text_field :email %><br><br>

  <%= f.submit %>
<% end %>
