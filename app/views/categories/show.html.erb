<div class='page-header'>
  <div class="btn-toolbar pull-right">
    <%= link_to "Return to all categories", categories_path, class: "btn btn-default" %>
  </div>
  <h3><%= @category.title %></h3>
  <hr>
</div>

<div class="col-md-12">
  <ul>
    <% @category.jobs.each do |job| %>
      <h4><%= link_to "#{job.title} at #{job.company.name}", company_job_path(job.company, job) %><h4>
    <% end %><br><br>
  </ul>
</div>




